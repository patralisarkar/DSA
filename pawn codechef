#include<iostream>
using namespace std
int main()
{

while(n--)
{int h=0;
    cin>>x;
    cin>>y;
    for(j=0;j<x;j++)
    {
        cin>>a[j];
    }
    for(j=0;j<x;j++)
    {if(y%a[j]==0)
    {h++;
        c=0;int h=a[j];
    int g=a[j];
     while(g<=y)
     {
         g=g+h;
         c++;
     }
     if(c-1>=1)
     {b[k]=c-1;
     min[k]=a[j];k++;}
    }
    }
    
    if(h>0){int f=min[0];
    for(i=0;i<k;i++)
    {
    if(b[i]>b[i+1])
    {
        f=min[i+1];

    }
}cout<<f<<endl;
    }
else
{
    cout<<"-1";
}

}return 0;
}

